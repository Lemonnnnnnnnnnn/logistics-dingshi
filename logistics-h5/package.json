{
  "name": "ant-design-pro",
  "version": "2.1.1",
  "description": "An out-of-box UI solution for enterprise applications",
  "private": true,
  "scripts": {
    "start": "cross-env APP_TYPE=site umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "dev": "copyfiles -f ./filters/development/envConfig.js ./src && cross-env APP_TYPE=site umi dev",
    "dev:out": "copyfiles -f ./filters/development_out/envConfig.js ./src && cross-env APP_TYPE=site umi dev",
    "dev:pre": "copyfiles -f ./filters/preproduction/envConfig.js ./src && cross-env APP_TYPE=site umi dev",
    "dev:pro": "copyfiles -f ./filters/production/envConfig.js ./src && cross-env APP_TYPE=site umi dev",
    "dev:no-mock": "cross-env MOCK=none umi dev",
    "build": "copyfiles -f ./filters/production/envConfig.js ./src && umi build",
    "build:pre": "copyfiles -f ./filters/preproduction/envConfig.js ./src && umi build",
    "build:test": "copyfiles -f ./filters/development_out/envConfig.js ./src && umi build",
    "build:pro": "copyfiles -f ./filters/production/envConfig.js ./src && umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "node ./scripts/prettier.js"
  },
  "dependencies": {
    "@antv/data-set": "^0.10.2",
    "@gem-mine/mobile-schema-form": "^1.0.16",
    "ali-oss": "^6.1.0",
    "ant-design-pro": "^2.3.2",
    "antd": "^3.10.9",
    "antd-mobile": "^2.2.13",
    "bizcharts": "^3.5.5",
    "bizgoblin": "^0.2.0",
    "classnames": "^2.2.6",
    "dva": "^2.4.0",
    "fastclick": "^1.0.6",
    "intl": "^1.2.5",
    "js-base64": "^2.5.1",
    "jsencrypt": "^3.0.0-rc.1",
    "lodash": "^4.17.15",
    "lodash-decorators": "^6.0.0",
    "memoize-one": "^4.0.0",
    "moment": "^2.22.2",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.5.10",
    "qrcode.react": "^1.0.0",
    "rc-animate": "^2.4.4",
    "rc-form": "^2.4.5",
    "react": "^16.13.1",
    "react-amap": "^1.2.8",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-css-modules": "^4.7.9",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-image-gallery": "^0.8.17",
    "react-imageview": "^1.2.1",
    "react-keep-alive": "^2.1.0",
    "react-media": "^1.8.0",
    "react-multi-carousel": "^1.4.1",
    "react-redux": "^6.0.1",
    "react-router-breadcrumbs-hoc": "^3.1.1",
    "react-wx-images-viewer": "^1.0.6",
    "react-zmage": "^0.8.4",
    "reactjs-swiper": "^0.1.0",
    "redux-actions": "^2.6.5",
    "regenerator-runtime": "^0.13.2",
    "scriptjs": "^2.5.9",
    "umi": "^2.8.16",
    "umi-plugin-locale": "^2.8.1",
    "umi-request": "^1.0.0",
    "video.js": "^7.15.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-import": "^1.12.2",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "dva-logger": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "1.15.2",
    "stylelint": "^9.8.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.0.0",
    "umi-plugin-lodash": "0.0.1",
    "umi-plugin-pro-block": "^1.2.0",
    "umi-plugin-react": "^1.9.14",
    "video.js": "^7.15.4"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/lint-prettier.js",
      "git add"
    ],
    "src/**/*.{js,jsx}": "npm run lint-staged:js",
    "src/**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
